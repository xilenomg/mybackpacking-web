<h1>Listing admin_checkins</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_checkins.each do |admin_checkin| %>
      <tr>
        <td><%= "#{admin_checkin.user.id} - #{admin_checkin.user.name}" %></td>
        <td><%= "#{admin_checkin.place.country} - #{admin_checkin.place.state} - #{admin_checkin.place.city} - #{admin_checkin.place.place_name}" %></td>
        <td><%= link_to 'Show', admin_checkin %></td>
        <td><%= link_to 'Edit', edit_admin_checkin_path(admin_checkin) %></td>
        <td><%= link_to 'Destroy', admin_checkin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Checkin', new_admin_checkin_path %>
